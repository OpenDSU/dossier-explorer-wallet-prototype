{
    "defaultMenuConfig": {
        "icon": "fa-bars",
        "type": "route",
        "component": "psk-page-loader",
        "exact": true,
        "indexed": true,
        "historyType": "browser"
    },
    "pages": [
        {
            "name": "Explorer",
            "path": "/explorer",
            "pageSrc": "index.html"
        },
        {
            "name": "Marketplaces",
            "children": [
                {
                    "name": "Marketplace Manager",
                    "path": "/marketplace-manager",
                    "pageSrc": "marketplace-manager.html",
                    "tags": "marketplace-manager"

                },
                {
                    "name": "Submit Marketplace",
                    "tags": "submit-marketplace",
                    "indexed": false
                },
                {
                    "name": "Marketplace List",
                    "path": "/marketplaces",
                    "children": "@event:getMarketplaces",
                    "component": "psk-ssapp",
                    "propName": "keySSI"
                }
            ]
        },
        {
            "name": "My Apps",
            "path": "/my-apps",
            "icon": "fa-plug",
            "children": "@event:getSSApps",
            "propName": "keySSI",
            "component": "psk-ssapp"
        }
    ]
}
