{
    "defaultMenuConfig": {
        "icon": "fa-bars",
        "type": "route",
        "component": "psk-page-loader",
        "exact": true,
        "indexed": true,
        "historyType": "browser"
    },
    "pages": [
        {
            "name": "Explorer",
            "path": "/explorer",
            "pageSrc": "index.html"
        },
        {
            "name": "Marketplace",
            "path": "/marketplace",
            "pageSrc": "marketplace.html"
        },
        {
            "name": "My Apps",
            "path": "/my-apps",
            "icon": "fa-plug",
            "children": "@event:getSSApps",
            "propName": "appName",
            "component": "psk-ssapp"
        },
        {
            "name": "MarketplacePage",
            "path": "/marketplace-page",
            "pageSrc": "Wallet/marketplace.html"
        },
        {
            "name": "Submit Application",
            "path": "/submit-application",
            "pageSrc": "Wallet/submit-application.html"
        },
        {
            "name": "Accept Applications",
            "path": "/accept-applications",
            "pageSrc": "Wallet/accept-applications.html"
        }
    ]
}
